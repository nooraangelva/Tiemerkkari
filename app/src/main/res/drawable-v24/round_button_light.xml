<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="false" >
        <shape>
            <gradient
                android:startColor="#275568C5"
                android:endColor="#413A4787"
                android:angle="270" />
            <stroke
                android:width="1px"
                android:color="#050505" />
            <corners
                android:radius="15dp" />
            <padding
                android:left="10dp"
                android:top="10dp" android:right="10dp"
                android:bottom="10dp" />

        </shape>
    </item>
    <item android:state_pressed="true" >
        <shape>
            <gradient
            android:startColor="#795568C5"
            android:endColor="#793A4787"
            android:angle="270" />
            <stroke
                android:width="1px"
                android:color="#050505" />
            <corners
                android:radius="15dp" />
            <padding
                android:left="10dp"
                android:top="10dp" android:right="10dp"
                android:bottom="10dp" />
        </shape>
    </item>
</selector>
