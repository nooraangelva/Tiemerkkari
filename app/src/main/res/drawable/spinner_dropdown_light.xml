<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient
                android:startColor="#A5B6F3"
                android:endColor="#8190D1"
                android:angle="270" />
            <stroke
                android:width="1px"
                android:color="#050505" />
            <corners
                android:radius="15dp" />
            <padding
                android:left="10dp"
                android:top="10dp" android:right="10dp"
                android:bottom="10dp" />

        </shape>
    </item>
</selector>
